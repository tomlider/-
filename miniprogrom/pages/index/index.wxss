page {
  width: 100%;
}
.swiper_container {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  height: 40rpx;
  background-color: #E6F7FF;
}
.swiper_item {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 25rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: 400;
  color: #00BFFF;
}
.black {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #000;
  opacity: 0.5;
  overflow: scroll;
  z-index: 1;
}
.bag1 {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.bag1 image {
  border: solid 1rpx;
  height: 60%;
  position: fixed;
  z-index: 999;
  top: 15%;
}
.bag1 .icon1 {
  position: fixed;
  z-index: 999;
  margin-top: 80vh;
}
.iop {
  height: 40vh;
  width: 750rpx;
}
.iop .swip {
  width: 100%;
}
.iop .swip .yui {
  width: 100%;
}
scroll-view {
  background-color: #f5f5f5;
  height: 100%;
}
scroll-view .banner {
  padding-top: 80rpx;
  padding-bottom: 80rpx;
  display: flex;
  width: 80%;
  background-color: white;
  height: 90%;
  border-radius: 5%;
  justify-content: space-around;
  align-items: center;
  margin: 20rpx auto;
}
scroll-view .banner .demo {
  display: flex;
  flex-direction: column;
}
scroll-view .banner .demo image {
  height: 100rpx;
  background-color: white;
}
scroll-view .banner .demo .text {
  color: black;
  font-size: 30rpx;
  font-weight: 600;
  text-align: center;
  padding-top: 30rpx;
}
scroll-view .banner .zhu {
  margin: auto 0;
  width: 2rpx;
  height: 100rpx;
  background-color: black;
  border: black 2rpx solid;
}
scroll-view .advert {
  width: 100%;
  background: #f5f5f5;
  position: relative;
}
scroll-view .tab-right {
  width: 100%;
  height: 60rpx;
}
scroll-view .advert .content-item text {
  width: 100%;
  line-height: 60rpx;
  font-size: 28rpx;
  color: #00BFFF;
  text-align: center;
  padding-left: 20rpx;
  box-sizing: border-box;
}
scroll-view .bottom {
  background-color: #f5f5f5;
  height: 100%;
  width: 95%;
  margin: 0 auto;
}
scroll-view .bottom .tuijian .ttext {
  display: flex;
  width: 100%;
  padding: 20rpx 0;
  height: 100%;
  align-items: center;
  background-color: white;
}
scroll-view .bottom .tuijian .ttext .tuijtex {
  font-size: 35rpx;
  font-weight: 700;
}
scroll-view .bottom .tuijian .ttext .quanb {
  margin-left: auto;
  font-size: 25rpx;
  color: #3a1b1b;
}
scroll-view .bottom .tuijian .kuai {
  display: flex;
  height: 300rpx;
  background-color: white;
}
scroll-view .bottom .tuijian .kuai .block {
  background-color: white;
  margin: 15rpx 20rpx 20rpx 0;
  border: 1rpx #f5f5f5 solid;
  display: flex;
  flex-direction: column;
  width: 300rpx;
}
scroll-view .bottom .tuijian .kuai .block .imad {
  height: 300rpx;
  width: 300rpx;
}
scroll-view .bottom .tuijian .kuai .block .tuitext {
  display: flex;
  padding: 15rpx 0;
}
scroll-view .bottom .tuijian .kuai .block .tuitext .tuiname {
  font-size: 30rpx;
  font-weight: 700;
  padding-left: 20rpx;
}
scroll-view .bottom .tuijian .kuai .block .tuitext .tuiprice {
  margin-left: auto;
  font-size: 35rpx;
  font-weight: 600;
  color: #00BFFF;
}
scroll-view .bottom .youh {
  margin-top: 30rpx;
  background-color: #f5f5f5;
}
scroll-view .bottom .youh .yhBan {
  display: flex;
  background-color: white;
  align-items: center;
  margin-top: 15rpx;
}
scroll-view .bottom .youh .yhBan .tou {
  width: 13%;
  font-size: 40rpx;
  color: #FF59B4;
  margin: 20rpx 20rpx 20rpx 40rpx;
}
scroll-view .bottom .youh .yhBan .yhIt {
  margin: 15rpx 20rpx;
  width: 50%;
  border-right: #FF59B4 1rpx dotted;
}
scroll-view .bottom .youh .yhBan .yhIt .yhT {
  font-size: 30rpx;
}
scroll-view .bottom .youh .yhBan .yhIt .yhzb {
  font-size: 25rpx;
  color: #B7B7B7;
}
scroll-view .bottom .youh .yhBan .yhw {
  margin: 10rpx 20rpx;
  font-size: 30rpx;
  color: #FF59B4;
}
scroll-view .bottom .youh .yuBott {
  margin: 5rpx auto;
  color: white;
  width: 60%;
  font-size: 30rpx;
  text-align: center;
  line-height: 60rpx;
  height: 60rpx;
  border-radius: 30rpx 0rpx;
  background-color: #FF59B4;
}
scroll-view .bottom .sjn {
  font-size: 40rpx;
  font-weight: 650;
  margin: 40rpx 0;
}
scroll-view .bottom .sjt {
  border-bottom: 1rpx #f5f5f5 solid;
}
scroll-view .bottom .sjt .sjtime {
  font-size: 30rpx;
  margin: 30rpx 0;
}
scroll-view .bottom .sjt .timeI {
  color: #B7B7B7;
  font-size: 30rpx;
}
scroll-view .bottom .address .addt {
  font-size: 30rpx;
  margin: 30rpx 0;
}
scroll-view .bottom .address .addb {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
scroll-view .bottom .address .addb .addi {
  font-size: 30rpx;
}
scroll-view .bottom .tocall {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
scroll-view .bottom .tocall .callt {
  font-size: 30rpx;
  margin: 30rpx 0;
}
.img {
  height: 35rpx;
  width: 35rpx;
  margin-right: 20rpx;
}
.backw {
  background-color: white;
}
