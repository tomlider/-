page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.cates image {
  height: 250rpx;
  width: 100%;
}
.cates .topbar {
  opacity: 0.8;
  background-color: white;
  width: 90%;
  height: 200rpx;
  border-radius: 20rpx;
  position: absolute;
  top: 100rpx;
  left: 5%;
}
.cates .zuj {
  position: relative;
  top: 50rpx;
}
.cates .contoier {
  display: flex;
  margin-top: 60rpx;
  height: calc(100vh - 370rpx);
}
.cates .contoier .left {
  background-color: #f5f5f5;
  height: auto;
  flex: 2;
}
.cates .contoier .left .cbox {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 25rpx;
  border: solid 3rpx white;
  color: #808080;
}
.cates .contoier .left .active {
  border-left: 5rpx solid #00BFFF;
  background-color: white;
  color: black;
  font-weight: 700;
}
.cates .contoier .right {
  background-color: white;
  height: 100%;
  flex: 6;
  display: flex;
  flex-direction: column;
  padding-left: 20rpx;
}
.cates .contoier .right .rightCates {
  margin-bottom: 100rpx;
}
.cates .contoier .right .rightCates .title {
  padding: 10rpx;
  font-weight: 700;
  text-align: left;
  font-size: 30rpx;
}
.cates .contoier .right .rightCates .block {
  padding: 15rpx 0;
  display: flex;
}
.cates .contoier .right .rightCates .block .iamg image {
  border-radius: 15rpx;
  width: 200rpx;
  height: 200rpx;
}
.cates .contoier .right .rightCates .block .self {
  display: flex;
  flex-direction: column;
  padding-left: 20rpx;
  justify-content: space-around;
  width: 100%;
}
.cates .contoier .right .rightCates .block .self .name {
  font-size: 28rpx;
  font-weight: 600;
}
.cates .contoier .right .rightCates .block .self .banBttom {
  display: flex;
  justify-content: space-between;
  width: 100%;
}
.cates .contoier .right .rightCates .block .self .banBttom .price {
  color: red;
  font-weight: 650;
  font-size: 28rpx;
}
.cates .contoier .right .rightCates .block .self .banBttom .jisuan {
  display: flex;
  padding-right: 20rpx;
  flex-direction: row-reverse;
}
.cates .contoier .right .rightCates .block .self .banBttom .jisuan .selt {
  font-size: 20rpx;
  text-align: center;
  line-height: 45rpx;
  height: 45rpx;
  width: 86rpx;
  border-radius: 25rpx;
  background-color: #00BFFF;
  color: white;
  padding-left: auto;
}
.cates .contoier .right .rightCates .block .self .banBttom .jisuan .addimg image {
  height: 45rpx;
  width: 45rpx;
}
.cates .contoier .right .rightCates .block .self .banBttom .jisuan .number {
  padding-left: 10rpx;
  padding-right: 10rpx;
  font-size: 28rpx;
}
.cates .contoier .right .rightCates .block .self .tui {
  font-size: 24rpx;
  color: #808080;
  overflow: hidden;
}
.cates .bottm {
  z-index: 999;
  width: 100%;
  display: flex;
  position: fixed;
  background-color: white;
  bottom: 0;
  opacity: 0.95;
  height: 80rpx;
  align-items: center;
  padding-top: auto;
}
.cates .bottm .cartimg {
  align-items: center;
  padding-bottom: 0;
  padding-left: 40rpx;
  width: 70rpx;
  height: 70rpx;
}
.cates .bottm .bank {
  padding-left: 30rpx;
  color: black;
  font-weight: 650;
  padding-top: 7rpx;
  font-size: 35rpx;
}
.cates .bottm .toactive {
  color: white;
  margin-right: 30rpx;
  width: 130rpx;
  font-size: 30rpx;
  margin-left: auto;
  text-align: center;
  line-height: 65rpx;
  height: 65rpx;
  border-radius: 50rpx;
  background-color: grey;
}
.cates .bottm .button {
  color: white;
  margin-right: 30rpx;
  width: 130rpx;
  font-size: 30rpx;
  margin-left: auto;
  text-align: center;
  line-height: 65rpx;
  height: 65rpx;
  border-radius: 50rpx;
  background-color: #00BFFF;
}
.Mcart {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0.6;
  background: #000;
  z-index: 666;
}
.zhuTi {
  position: fixed;
  width: 100%;
  margin-bottom: 80rpx;
  bottom: 0;
  background-color: white;
  left: 0;
  z-index: 777;
  max-height: 600rpx;
  overflow: scroll;
}
.zhuTi .youhui {
  height: 30rpx;
  line-height: 30rpx;
  background-color: #f7efda;
  text-align: center;
  font-size: 30rpx;
  top: 0;
  position: sticky;
  z-index: 888;
}
.zhuTi .second {
  display: flex;
  top: 30rpx;
  position: sticky;
  z-index: 999;
  background-color: white;
}
.zhuTi .second .clearto {
  margin-left: auto;
  margin-right: 20rpx;
}
.zhuTi .second .clearto image {
  width: 30rpx;
}
.zhuTi .second .clearto .cleart {
  font-size: 30rpx;
  padding-left: 5rpx;
  color: #B7B7B7;
}
.zhuTi scroll-view {
  height: 80%;
  flex-direction: column;
}
.zhuTi scroll-view .cblock {
  height: 150rpx;
  display: flex;
  padding-left: 20rpx;
}
.zhuTi scroll-view .cblock .ciamg {
  padding: 10rpx 0;
}
.zhuTi scroll-view .cblock .ciamg image {
  width: 150rpx;
  height: 100%;
  border-radius: 10rpx;
}
.zhuTi scroll-view .cblock .cself {
  display: flex;
  flex-direction: column;
  padding-left: 20rpx;
  justify-content: space-around;
  width: 100%;
}
.zhuTi scroll-view .cblock .cself .cname {
  font-size: 30rpx;
  font-weight: 600;
}
.zhuTi scroll-view .cblock .cself .cbI {
  display: flex;
  text-overflow: ellipsis;
}
.zhuTi scroll-view .cblock .cself .cbI .cBei {
  font-size: 30rpx;
  color: #B7B7B7;
  margin-right: 10rpx;
}
.zhuTi scroll-view .cblock .cself .cbanBttom {
  display: flex;
  justify-content: space-between;
  width: 100%;
}
.zhuTi scroll-view .cblock .cself .cbanBttom .price {
  color: red;
  font-weight: 650;
  font-size: 28rpx;
}
.zhuTi scroll-view .cblock .cself .cbanBttom .cjisuan {
  display: flex;
  padding-right: 20rpx;
  flex-direction: row-reverse;
}
.zhuTi scroll-view .cblock .cself .cbanBttom .cjisuan .caddimg image {
  height: 45rpx;
  width: 45rpx;
}
.zhuTi scroll-view .cblock .cself .cbanBttom .cjisuan .cnumber {
  padding-left: 10rpx;
  padding-right: 10rpx;
  font-size: 28rpx;
}
.xuanM {
  opacity: 0.95;
  background-color: white;
  position: fixed;
  width: 80%;
  height: 60%;
  margin: auto;
  left: 0;
  right: 0;
  top: 120rpx;
  bottom: 0;
  z-index: 999;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  padding-top: 50rpx;
}
.xuanM .cancel {
  position: absolute;
  right: 10rpx;
  top: 10rpx;
}
.xuanM .xname {
  text-align: center;
  font-size: 40rpx;
  font-weight: 700;
}
.xuanM .cjisuan {
  display: flex;
  justify-content: space-between;
  padding: 40rpx 30rpx;
}
.xuanM .cjisuan .shut {
  font-size: 30rpx;
  color: #00BFFF;
}
.xuanM .cjisuan .calu {
  display: flex;
}
.xuanM .cjisuan .calu .caddimg {
  padding-left: 10rpx;
}
.xuanM .cjisuan .calu .caddimg image {
  height: 45rpx;
  width: 45rpx;
}
.xuanM .cjisuan .calu .cnumber {
  padding-left: 10rpx;
  font-size: 35rpx;
}
.xuanM .kwmk {
  padding: 10rpx 15rpx;
}
.xuanM .kwmk .kt {
  font-size: 30rpx;
  color: #00BFFF;
}
.xuanM .kwmk .kmodol {
  display: flex;
  flex-wrap: wrap;
}
.xuanM .kwmk .kmodol .activ {
  background-color: #33CCFF;
  font-size: 30rpx;
  border-radius: 15rpx;
  margin: 9rpx 15rpx;
  padding: 15rpx 30rpx;
}
.xuanM .kwmk .kmodol .kbank {
  margin: 9rpx 15rpx;
  padding: 15rpx 30rpx;
  background-color: #f5f5f5;
  font-size: 30rpx;
  border-radius: 15rpx;
}
.xuanM .addTocart {
  position: absolute;
  bottom: 60rpx;
  align-self: center;
  width: 75%;
  height: 65rpx;
  border-radius: 30rpx;
  background-color: #00BFFF;
  color: white;
  font-size: 35rpx;
  line-height: 65rpx;
  text-align: center;
}
